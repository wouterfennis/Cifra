{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Cifra.Api"
    }
  },
  "AllowedHosts": "*",
  "Spreadsheet": {
    "TestResultsDirectory": "/data/TestResults"
  },
  "ConnectionStrings": {
    "Sqlite": "cifra.db"
  }
}
